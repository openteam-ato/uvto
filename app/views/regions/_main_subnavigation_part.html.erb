<div class="submenu_opened">
  <div class='inner'>
    <ul>
      <li class='subnavigation_title'><%= object.values.last.title %>:</li>
      <% object.values.last['children'].each do |name, item| %>
        <li <%= 'class=active' if item['selected'] %>>
          <%= link_to item['title'], item['path'] %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
